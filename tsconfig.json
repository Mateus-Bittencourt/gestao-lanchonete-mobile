{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "jsx": "react",
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": [
      "jest",
      "react",
      "react-native"
    ]
  },
  "include": ["src", "App.tsx", "**/*.ts", "**/*.tsx", "declarations.d.ts"]
}